{
  "name": "@gabrielrufino/pdf-studio-bot",
  "private": true,
  "version": "1.1.4",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "start": "node .",
    "start:dev": "tsx watch --env-file=.env src/index.ts | pino-pretty",
    "test": "vitest run",
    "test:cov": "npm test -- --coverage",
    "test:watch": "vitest",
    "prepare": "if [[ $NODE_ENV != \"production\" ]]; then husky; fi"
  },
  "author": "Gabriel Rufino <contato@gabrielrufino.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@faker-js/faker": "^8.4.1",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.34.0",
    "husky": "^9.1.7",
    "pino-pretty": "^13.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@grammyjs/files": "^1.1.1",
    "grammy": "^1.38.1",
    "mongodb": "^6.19.0",
    "muhammara": "^5.3.0",
    "pino": "^9.9.0",
    "puppeteer": "^24.18.0"
  }
}
