{
  "name": "@gabrielrufino/pdfstudio-bot",
  "private": true,
  "version": "1.0.3",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "start": "node .",
    "start:dev": "nodemon --ext ts --exec 'ts-node -r dotenv/config src/index.ts | pino-pretty'",
    "test": "vitest run",
    "test:cov": "npm test -- --coverage",
    "test:watch": "vitest",
    "prepare": "if [[ $NODE_ENV != \"production\" ]]; then husky; fi"
  },
  "author": "Gabriel Rufino <contato@gabrielrufino.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@faker-js/faker": "^8.4.1",
    "@types/pdfkit": "^0.13.8",
    "@vitest/coverage-v8": "^2.1.8",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@grammyjs/files": "^1.1.1",
    "@mikro-orm/core": "^6.4.2",
    "@mikro-orm/mongodb": "^6.4.2",
    "grammy": "^1.33.0",
    "muhammara": "^5.3.0",
    "pdfkit": "^0.15.2",
    "pino": "^9.6.0"
  }
}
